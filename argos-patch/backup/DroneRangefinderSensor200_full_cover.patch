+#define DRONE_RANGEFINDERS_DEFAULT_SENSOR_H
+
+namespace argos {
+   class CDroneRangefindersDefaultSensor;
+   class CControllableEntity;
+   class CEmbodiedEntity;
+   struct SAnchor;
+}
+
+#include <argos3/core/simulator/sensor.h>
